<div class="container">
    <h2>Cleaning order</h2>
    <div class="row">
        <div class="container col-md-6">
            <h3>Current cleaner</h3>
            <p>Week: <%= current.week %></p>
            <p>Name: <%= current.user.name %> </p>
            <p>From: <%= current.start %> </p>
            <p>To: <%= current.end %> </p>
        </div>
        <div class="container col-md-6">
            <h3>Next cleaner</h3>
            <p>Week: <%= nextCleaner.week %></p>
            <p>Name: <%= nextCleaner.name %> </p>
            <p>From: <%= nextCleaner.start %> </p>
            <p>To: <%= nextCleaner.end %> </p>
        </div>
    </div>
    
</div>

